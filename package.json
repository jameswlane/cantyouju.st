{
  "name": "cantyoujust-theme",
  "description": "GatsbyJS theme with the Casper theme from Ghost.",
  "version": "1.0.0",
  "author": "James W. Lane <james.w.lane@mac.com>",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:css": "stylelint --fix 'src/**/*.css'",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "release": "standard-version -a",
    "now-build": "npm run build"
  },
  "dependencies": {
    "gatsby-image": "2.2.29",
    "gatsby-plugin-typography": "2.3.14",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "gatsby": "^2.17.11",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-feed": "^2.3.19",
    "gatsby-plugin-google-analytics": "^2.1.23",
    "gatsby-plugin-lodash": "^3.1.13",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-nprogress": "^2.1.12",
    "gatsby-plugin-offline": "^2.2.6",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-sitemap": "^2.2.20",
    "gatsby-plugin-twitter": "^2.1.12",
    "gatsby-remark-autolink-headers": "^2.1.16",
    "gatsby-remark-copy-linked-files": "^2.1.28",
    "gatsby-remark-images": "3.1.25",
    "gatsby-remark-prismjs": "^3.3.20",
    "gatsby-remark-responsive-iframe": "^2.2.26",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-json": "^2.2.17",
    "gatsby-transformer-remark": "^2.6.37",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "prismjs": "^1.17.1",
    "react": "^16.11.0",
    "react-disqus-comments": "^1.2.0",
    "react-dom": "^16.10.2",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.1.3",
    "react-scroll": "^1.7.14",
    "react-share": "3.0.1",
    "react-social-icons": "4.1.0",
    "react-twitter-widgets": "^1.4.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1",
    "remark-cli": "7.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "standard-version": "7.0.0",
    "stylefmt": "^6.0.2",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "^18.2.0",
    "write-good": "1.0.2"
  },
  "keywords": [
    "gatsby",
    "blog",
    "casper",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/jameswlane/cantyouju.st/issues"
  },
  "homepage": "https://github.com/jameswlane/cantyouju.st#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameswlane/cantyouju.st.git"
  }
}
